{
  "manifest_version": 3,
  "name": "Leaflet Coords Logger",
  "version": "1.0",
  "description": "Logs clicked coordinates from a Leaflet map.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
