{
  "manifest_version": 3,
  "name": "Leaflet Coordinate Receiver",
  "version": "1.0",
  "description": "Logs coordinates from a Leaflet map when clicked.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
